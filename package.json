{
  "name": "conditionallogger",
  "displayName": "ConditionalLogger",
  "description": "Automatically inserts console.log() statements inside conditional blocks like if, else, and switch to make debugging faster and easier.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Debugging",
    "Other"
  ],
  "keywords": [
    "console.log",
    "conditional",
    "if else",
    "switch",
    "debugging",
    "logger",
    "auto log",
    "developer tools"
  ],
  "activationEvents": ["onCommand:conditionallogger.ConditionalLogger"],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "conditionallogger.ConditionalLogger",
      "title": "Insert Logs into Conditionals"
    }],
    "keybindings": [
      {
        "command": "conditionallogger.ConditionalLogger",
        "key": "alt+l",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.102.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.25.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
